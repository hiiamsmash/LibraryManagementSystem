1. INTERN_USER_LOGIN
sql
Copy
Edit
CREATE TABLE INTERN_USER_LOGIN (
    LOGIN_ID NUMBER PRIMARY KEY,
    USERNAME VARCHAR2(50) NOT NULL UNIQUE,
    PASSWORD VARCHAR2(50) NOT NULL
);

-- Dummy Data
INSERT INTO INTERN_USER_LOGIN (LOGIN_ID, USERNAME, PASSWORD) 
VALUES (1, 'ravi_kumar', 'password123');

INSERT INTO INTERN_USER_LOGIN (LOGIN_ID, USERNAME, PASSWORD) 
VALUES (2, 'priya_sharma', 'password456');
2. INTERN_ACCOUNT_TYPE
sql
Copy
Edit
CREATE TABLE INTERN_ACCOUNT_TYPE (
    ACCOUNT_TYPE_ID NUMBER PRIMARY KEY,
    ACCOUNT_TYPE VARCHAR2(50) NOT NULL,
    ACCOUNT_DESC VARCHAR2(100),
    ACCOUNT_TYPE_CODE VARCHAR2(10),
    ACCOUNT_TYPE_COUNTER NUMBER
);

-- Dummy Data
INSERT INTO INTERN_ACCOUNT_TYPE (ACCOUNT_TYPE_ID, ACCOUNT_TYPE, ACCOUNT_DESC, ACCOUNT_TYPE_CODE, ACCOUNT_TYPE_COUNTER)
VALUES (1, 'SAVINGS', 'Savings Account', 'SB', 1);

INSERT INTO INTERN_ACCOUNT_TYPE (ACCOUNT_TYPE_ID, ACCOUNT_TYPE, ACCOUNT_DESC, ACCOUNT_TYPE_CODE, ACCOUNT_TYPE_COUNTER)
VALUES (2, 'CURRENT', 'Current Account', 'CA', 1);
3. INTERN_CUSTOMER
sql
Copy
Edit
CREATE TABLE INTERN_CUSTOMER (
    CUSTOMER_ID NUMBER PRIMARY KEY,
    LOGIN_ID NUMBER UNIQUE,
    NAME VARCHAR2(100) NOT NULL,
    EMAIL VARCHAR2(100) NOT NULL,
    PHONE VARCHAR2(15),
    ADDRESS VARCHAR2(200),
    DOB DATE,
    ACCOUNT_TYPE_ID NUMBER NOT NULL,
    FOREIGN KEY (LOGIN_ID) REFERENCES INTERN_USER_LOGIN(LOGIN_ID),
    FOREIGN KEY (ACCOUNT_TYPE_ID) REFERENCES INTERN_ACCOUNT_TYPE(ACCOUNT_TYPE_ID)
);

-- Dummy Data
INSERT INTO INTERN_CUSTOMER (CUSTOMER_ID, LOGIN_ID, NAME, EMAIL, PHONE, ADDRESS, DOB, ACCOUNT_TYPE_ID)
VALUES (1, 1, 'Ravi Kumar', 'ravi@example.com', '9876543210', '123 Street, Delhi', TO_DATE('1990-05-20', 'YYYY-MM-DD'), 1);

INSERT INTO INTERN_CUSTOMER (CUSTOMER_ID, LOGIN_ID, NAME, EMAIL, PHONE, ADDRESS, DOB, ACCOUNT_TYPE_ID)
VALUES (2, 2, 'Priya Sharma', 'priya@example.com', '9876543211', '456 Street, Mumbai', TO_DATE('1985-08-15', 'YYYY-MM-DD'), 2);
4. INTERN_ACCOUNT_BALANCE
sql
Copy
Edit
CREATE TABLE INTERN_ACCOUNT_BALANCE (
    BALANCE_ID NUMBER PRIMARY KEY,
    CUSTOMER_ID NUMBER NOT NULL,
    BALANCE NUMBER(15, 2) DEFAULT 0.00,
    FOREIGN KEY (CUSTOMER_ID) REFERENCES INTERN_CUSTOMER(CUSTOMER_ID)
);

-- Dummy Data
INSERT INTO INTERN_ACCOUNT_BALANCE (BALANCE_ID, CUSTOMER_ID, BALANCE) 
VALUES (1, 1, 10000.00);  -- Balance for Ravi Kumar

INSERT INTO INTERN_ACCOUNT_BALANCE (BALANCE_ID, CUSTOMER_ID, BALANCE) 
VALUES (2, 2, 5000.00);   -- Balance for Priya Sharma
5. INTERN_BANK_TRANSACTION
sql
Copy
Edit
CREATE TABLE INTERN_BANK_TRANSACTION (
    TRANSACTION_ID NUMBER PRIMARY KEY,
    CUSTOMER_ID NUMBER NOT NULL,
    TRANSACTION_TYPE VARCHAR2(10) CHECK (TRANSACTION_TYPE IN ('DEPOSIT', 'WITHDRAW')),
    AMOUNT NUMBER(15, 2) NOT NULL,
    TRANSACTION_DATE DATE DEFAULT SYSDATE,
    FOREIGN KEY (CUSTOMER_ID) REFERENCES INTERN_CUSTOMER(CUSTOMER_ID)
);

-- Dummy Data
INSERT INTO INTERN_BANK_TRANSACTION (TRANSACTION_ID, CUSTOMER_ID, TRANSACTION_TYPE, AMOUNT, TRANSACTION_DATE)
VALUES (1, 1, 'DEPOSIT', 5000.00, TO_DATE('2025-05-01', 'YYYY-MM-DD'));

INSERT INTO INTERN_BANK_TRANSACTION (TRANSACTION_ID, CUSTOMER_ID, TRANSACTION_TYPE, AMOUNT, TRANSACTION_DATE)
VALUES (2, 1, 'WITHDRAW', 2000.00, TO_DATE('2025-05-02', 'YYYY-MM-DD'));

INSERT INTO INTERN_BANK_TRANSACTION (TRANSACTION_ID, CUSTOMER_ID, TRANSACTION_TYPE, AMOUNT, TRANSACTION_DATE)
VALUES (3, 2, 'DEPOSIT', 1000.00, TO_DATE('2025-05-03', 'YYYY-MM-DD'));
